sm_Insert_new_units:BindBySelect_GetQKFromDV
  a sml:BindBySelect ;
  sm:next sm_Insert_new_units:PerformUpdate_InsertNewInstances ;
  sm:nodeX 704 ;
  sm:nodeY 334 ;
  sml:selectQuery [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "qk" ;
          ]
          [
            sp:varName "dv" ;
          ]
        ) ;
      sp:where (
          [
            a sp:Bind ;
            sp:expression [
                a fn:concat ;
                sp:arg1 "qkdv:" ;
                sp:arg2 [
                    sp:varName "result" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "newDVName" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a smf:buildURI ;
                sp:arg1 [
                    sp:varName "newDVName" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "newDVQname" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "dv" ;
              ] ;
            sp:predicate qudt:hasDimensionVector ;
            sp:subject [
                sp:varName "qk" ;
              ] ;
          ]
          [
            a sp:Filter ;
            sp:expression [
                a sp:eq ;
                sp:arg1 [
                    sp:varName "newDVQname" ;
                  ] ;
                sp:arg2 [
                    sp:varName "dv" ;
                  ] ;
              ] ;
          ]
        ) ;
    ] ;
.
sm_Insert_new_units:BindBySelect_GetUnitInstances
  a sml:BindBySelect ;
  sm:next sm_Insert_new_units:PerformUpdate_InsertNewInstances ;
  sm:next sm_Insert_new_units:convertDimensions_1 ;
  sm:nodeX 319 ;
  sm:nodeY 233 ;
  sml:selectQuery [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "unit3" ;
          ]
        ) ;
      sp:where (
          [
            sp:object <http://qudt-public-repo/submissions/newUnits3#Export> ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "unit3" ;
              ] ;
          ]
        ) ;
    ] ;
.
